{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "yasqlplus",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/debug/yasqlplus",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [
                {
                    "name": "YASHANDB_HOST",
                    "value": "127.0.0.1"
                },
                {
                    "name": "YASHANDB_PORT",
                    "value": "1688"
                },
                {
                    "name": "YASHANDB_USERNAME",
                    "value": "sys"
                },
                {
                    "name": "YASHANDB_PASSWORD",
                    "value": "Cod-2022"
                },
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}